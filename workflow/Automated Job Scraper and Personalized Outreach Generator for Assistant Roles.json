        "inputSchema": "{\n\t\"type\": \"object\",\n\t\"properties\": {\n\t\t\"isRemote\": {\n\t\t\t\"type\": \"boolean\",\n\t\t\t\"description\": \"Whether the job is remote or allows remote work\"\n\t\t},\n\t\t\"isEnglishSpeaking\": {\n\t\t\t\"type\": \"boolean\",\n\t\t\t\"description\": \"Whether the job requires English language skills\"\n\t\t},\n\t\t\"isRelevant\": {\n\t\t\t\"type\": \"boolean\",\n\t\t\t\"description\": \"Whether the job is relevant for Executive Assistant or Admin Assistant role\"\n\t\t},\n\t\t\"reasoning\": {\n\t\t\t\"type\": \"string\",\n\t\t\t\"description\": \"Brief explanation of the decision\"\n\t\t}\n\t},\n\t\"required\": [\"isRemote\", \"isEnglishSpeaking\", \"isRelevant\", \"reasoning\"]\n}"
      },
